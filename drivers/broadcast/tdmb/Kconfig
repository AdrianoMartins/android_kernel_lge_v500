config LGE_BROADCAST_TDMB_FC8050
	bool "LGE broacast TDMB FC8050 device"
	depends on LGE_BROADCAST_TDMB && (MACH_APQ8064_J1U || MACH_APQ8064_J1KT || MACH_APQ8064_J1SK || MACH_APQ8064_GKU || MACH_APQ8064_GKKT || MACH_APQ8064_GKSK)
	default y
	help
	  Select this if you want to use broadcast service
	  
